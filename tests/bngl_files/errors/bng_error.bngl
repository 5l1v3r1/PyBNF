begin model

  begin parameters

# Original values used to generate parabola.exp
# (v1, v2, v3) = 0.5, 1, 3

#Parabola A:    Y = v1*(X^2)  +  v2*X   +  v3

    v1 v1__FREE__
    v2 v2__FREE__
    v3 v3__FREE__

  end parameters

  begin molecule types
    counter()
  end molecule types

  begin seed species
    # initial conditions
    counter() -10
  end seed species

  begin observables
    Molecules x counter()
  end observables

  begin functions
 #   y()=(x-v1)*(x-v2)*(x-v3)*(x-p4)*(x-p5)+1000

#    y()=(v1*x^2)+(v2*x)+v3
    y()=v1*(x^2)+(v2*x)+v3
  end functions

  begin reaction rules
    0->counter() 1
    0->A(x) # Invalid
  end reaction rules

end model

begin actions
  generate_network({overwrite=>1})
  simulate({method=>"ode",t_start=>-10,t_end=>10,n_steps=>20,suffix=>"par1",print_functions=>1})

end actions


